{
  "name": "cnpm-user-service-gitlab",
  "version": "0.0.4",
  "description": "gitlab user service for cnpmjs.org",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --opts ./test/mocha.opts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "eslint": "eslint ./src",
    "autod": "autod -w --prefix '~'",
    "cnpm": "npm install --registry=https://registry.npm.taobao.org",
    "contributors": "contributors -f plain -o AUTHORS",
    "build": "babel src --out-dir lib"
  },
  "dependencies": {
    "form-data": "^2.1.4",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "co-mocha": "^1.2.0",
    "autod": "*",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "contributors": "*",
    "cov": "*",
    "coveralls": "^2.13.1",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "istanbul-harmony": "*",
    "mocha": "^3.3.0",
    "nyc": "^10.2.0",
    "should": "~4.4.2"
  },
  "homepage": "https://github.com/ltchronus/npm-user-service",
  "repository": {
    "type": "git",
    "url": "git://github.com/ltchronus/npm-user-service.git",
